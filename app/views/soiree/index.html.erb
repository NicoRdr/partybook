<h1>Liste Soirées</h1>


<% @soirees.each do |soiree|%>
 <ul class="list-unstyled"> 
  <li ><%= soiree.nom%></li>
  <li ><%= soiree.date%></li>
  <li ><%= soiree.type_of%></li>
  <li ><%= soiree.description%></li>
  <li ><%= soiree.prix %>€</li>
  <li ><%= soiree.lieux%></li>
  
</ul>
<%= link_to "Voir la soirée", soiree_path%>
<%= link_to "crée une soirée", soiree_path%>
<% end %>

